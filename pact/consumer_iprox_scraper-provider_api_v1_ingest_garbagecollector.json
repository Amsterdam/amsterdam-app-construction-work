{
  "consumer": {
    "name": "Consumer_iprox_scraper"
  },
  "provider": {
    "name": "Provider_api_v1_ingest_garbagecollector"
  },
  "interactions": [
    {
      "description": "a request for garbage collection",
      "providerState": "Consumer calls Garbage Collector route on backend",
      "request": {
        "method": "get",
        "path": "/api/v1/ingest/garbagecollector",
        "query": "project_type=brug",
        "headers": {
          "Accept": "application/json",
          "IngestAuthorization": "a/secret/token"
        },
        "matchingRules": {
          "$.headers.IngestAuthorization": {
            "match": "type"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
        },
        "body": {
          "status": true,
          "result": "Garbage collection done"
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}